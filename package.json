{
  "name": "angular-nest-starter",
  "version": "0.1.2",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules package-lock.json",
    "hmr": "concurrently \"nodemon \" \"ng serve --hmr --progress=false --proxy-config proxy.conf.json\" ",
    "dev": "concurrently \"nodemon \" \"ng serve --progress=false --proxy-config proxy.conf.json\"  ",
    "ng": "ng",
    "prestart": "npm run build",
    "start": "set NODE_ENV=production && node dist/server/main.js",
    "build": "ng build --prod && tsc -p ./server",
    "test": "ng test",
    "test:backend": "jest --config=server/jest.json",
    "test:backend:watch": "jest --watch --config=server/jest.json",
    "test:backend:coverage": "jest --config=server/jest.json --coverage --coverageDirectory=coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:backend": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:backend:watch": "jest --watch --config=e2e/jest-e2e.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.2",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/http": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/platform-browser-dynamic": "6.0.2",
    "@angular/router": "6.0.2",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "core-js": "^2.5.6",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "^6.0.1",
    "@angular/compiler-cli": "6.0.2",
    "@angular/language-service": "6.0.2",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^22.1.2",
    "@types/node": "~10.1.0",
    "codelyzer": "^4.3.0",
    "concurrently": "^3.5.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.3",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "nodemon": "^1.17.4",
    "protractor": "~5.3.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  }
}
